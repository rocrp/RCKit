{
    "version": 1,
    "indentation": {
        "spaces": 4
    },
    "lineLength": 140,
    "maximumBlankLines": 1,
    "respectsExistingLineBreaks": true,
    "lineBreakBeforeEachArgument": true,
    "multiElementCollectionTrailingCommas": true,
    "spacesAroundRangeFormationOperators": false,
    "rules": {
        "AlwaysUseLowerCamelCase": false
    }
}
